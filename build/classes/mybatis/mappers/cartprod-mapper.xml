<?xml version="1.0" encoding="UTF-8"?>
<!-- 이 문서는 maBatis에서 처리할 SQL문을 작성하는 문서 입니다.-->
<!DOCTYPE mapper
	PUBLIC "-//mybatis.org/DTD Mapper3.0//EN"
   "http://mybatis.org/dtd/mybatis-3-mapper.dtd"> 
   
 
<mapper namespace="cartprod"> 
	<select id="getProdId" parameterType="int" resultType="cartprodVo">
		select * from cart_prod where cart_no = #{cart_no}
	</select>

</mapper>